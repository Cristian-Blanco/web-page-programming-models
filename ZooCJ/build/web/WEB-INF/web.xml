<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>PlanDirector</servlet-name>
        <servlet-class>servlets.PlanDirector</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SavePlan</servlet-name>
        <servlet-class>servlets.SavePlan</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Animals</servlet-name>
        <servlet-class>servlets.Animals</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Instalations</servlet-name>
        <servlet-class>servlets.Instalations</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PlanDirector</servlet-name>
        <url-pattern>/PlanDirector</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SavePlan</servlet-name>
        <url-pattern>/SavePlan</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Animals</servlet-name>
        <url-pattern>/Animals</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Instalations</servlet-name>
        <url-pattern>/Instalations</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
